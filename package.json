{
  "name": "saku-store-clean",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:secure": "powershell -Command \"& { .\\scripts\\setup-dev-firewall.ps1 -Enable; npm run dev }\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "preview": "next build && next start",
    "postbuild": "next-sitemap",
    "security:enable": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-dev-firewall.ps1 -Enable",
    "security:disable": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-dev-firewall.ps1 -Disable",
    "security:status": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-dev-firewall.ps1 -Status",
    "supabase:start": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-supabase-local.ps1 -Start",
    "supabase:stop": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-supabase-local.ps1 -Stop",
    "supabase:reset": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-supabase-local.ps1 -Reset",
    "supabase:status": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-supabase-local.ps1 -Status",
    "db:migrate": "supabase db reset",
    "db:seed": "supabase db reset --with-seed",
    "test:e2e": "playwright test --reporter=list",
    "test:e2e:ci": "playwright test --reporter=list --reporter=html",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report --host=127.0.0.1 --port=0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.90.1",
    "@types/nodemailer": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
